{
  "name": "Cosmic Planner",
  "short_name": "CosmicPlanner",
  "description": "Daily planner with sci-fi aesthetics and anime tracking",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}